file = {::},
issn = {1473-5504},
journal = {International Journal of Astrobiology},
keywords = {Kant,Wager,belief,extra-terrestrial life,history,plurality of worlds,theology},
month = {oct},
number = {04},
pages = {261--270},
publisher = {Cambridge University Press},
title = {{Kant's wager. Kant's strong belief in extra-terrestrial life, the history of this question and its challenge for theology today}},
url = {http://www.journals.cambridge.org/abstract{\_}S1473550416000112},
volume = {15},
year = {2016}
}
