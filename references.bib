month = {jul},
number = {1},
pages = {1--12},
pmid = {994183},
publisher = {Academic Press},
title = {{The nature of the accessible and buried surfaces in proteins}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0022283676901911},
volume = {105},
year = {1976}
}
