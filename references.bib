journal = {Nucleic Acids Research},
keywords = {Algorithms,Carbohydrate Sequence,Internet,Molecular Sequence Data,Polysaccharides,Polysaccharides: chemistry,Sequence Alignment,Software,User-Computer Interface},
month = {jul},
number = {WEB SERVER ISS.},
pages = {267--272},
pmid = {15215393},
title = {{KCaM (KEGG Carbohydrate Matcher): A software tool for analyzing the structures of carbohydrate sugar chains}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=441611{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {32},
year = {2004}
}
@article{Hattori2010,
abstract = {One of the greatest challenges in bioinformatics is to shed light on the relationship between genomic and chemical significances of metabolic pathways. Here, we demonstrate two types of chemical structure search servers: SIMCOMP (http://www.genome.jp/tools/simcomp/) for the chemical similarity search and SUBCOMP (http://www.genome.jp/tools/subcomp/) for the chemical substructure search, where both servers provide links to the KEGG PATHWAY and BRITE databases. The SIMCOMP is a graph-based method for searching the maximal common subgraph isomorphism by finding the maximal cliques in the association graph. In contrast, the SUBCOMP is an extended method for solving the subgraph isomorphism problem. The obtained links to PATHWAY or BRITE databases can be used to interpret as the biological meanings of chemical structures from the viewpoint of the various biological functions including metabolic networks.},
author = {Hattori, Masahiro and Tanaka, Nobuya and Kanehisa, Minoru and Goto, Susumu},
