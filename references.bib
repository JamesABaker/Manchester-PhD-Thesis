pmid = {16115865},
title = {{Lipids as modulators of proteolytic activity of BACE: Involvement of cholesterol, glycosphingolipids, and anionic phospholipids in vitro}},
url = {http://www.jbc.org/cgi/doi/10.1074/jbc.M504484200},
volume = {280},
