journal = {British Medical Journal},
keywords = {Cacao,Candy,Candy: statistics {\&} numerical data,Feeding Behavior,Hospital,Hospital Departments,Hospital Departments: statistics {\&} numerical data,Hospital: statistics {\&} numerical data,Humans,Inpatients,Inpatients: statistics {\&} numerical data,Personnel,Prospective Studies,Time Factors},
