journal = {Journal of Molecular Biology},
keywords = {POPC, palmitoyloleoylphosphatidylcholine,SnD, snorkeling distance,SnP, snorkeling propensity,TM, transmembrane,membrane,polarity,protein,rotamer,side-chain},
mendeley-groups = {Continuation report,PhD manuscript},
month = {may},
number = {2},
pages = {471--479},
pmid = {15136048},
title = {{Snorkeling preferences foster an amino acid composition bias in transmembrane helices}},
