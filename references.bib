issn = {03051048},
journal = {Nucleic Acids Research},
keywords = {Gene Expression Profiling,Gene Regulatory Networks,Internet,Phylogeny,Proteins,Proteins: analysis,Proteins: genetics,Software,User-Computer Interface},
month = {jul},
number = {W1},
pages = {W162--W167},
pmid = {22610856},
title = {{GENIES: Gene network inference engine based on supervised analysis}},
