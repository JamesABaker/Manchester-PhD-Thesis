journal = {Molecular Membrane Biology},
keywords = {Animals,Cell Membrane,Cell Membrane: metabolism,Humans,Hydrophobic and Hydrophilic Interactions,Lipid Bilayers,Lipid Bilayers: chemistry,Lipid Bilayers: metabolism,Membrane Lipids,Membrane Lipids: chemistry,Membrane Lipids: metabolism,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: metabolism,Protein Conformation},
language = {en},
