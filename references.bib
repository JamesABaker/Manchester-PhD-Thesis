issn = {1064-3745},
keywords = {Computer Simulation,Kinetics,Lipid Bilayers,Lipid Bilayers: metabolism,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: metabolism,Models,Molecular},
pages = {147--60},
