number = {36},
pages = {10833--10842},
pmid = {12962508},
title = {{The effect of interactions involving ionizable residues flanking membrane-inserted hydrophobic helices upon helix-helix interaction}},
url = {http://pubs.acs.org/doi/abs/10.1021/bi034929i},
volume = {42},
year = {2003}
}
