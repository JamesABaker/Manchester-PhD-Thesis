file = {:home/james/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sato, Hresko, Mueckler - 1998 - Testing the charge difference hypothesis for the assembly of a eucaryotic multispanning membrane protein.pdf:pdf},
