pages = {3366--3375},
pmid = {27807216},
title = {{Collective strategy for obstacle navigation during cooperative transport by ants}},
url = {http://jeb.biologists.org/lookup/doi/10.1242/jeb.143818},
volume = {219},
year = {2016}
}
