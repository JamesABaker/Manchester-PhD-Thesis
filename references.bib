@article{Obrezanova2015,
abstract = {Aggregation is a common problem affecting biopharmaceutical development that can have a significant effect on the quality of the product, as well as the safety to patients, particularly because of the increased risk of immune reactions. Here, we describe a new high-throughput screening algorithm developed to classify antibody molecules based on their propensity to aggregate. The tool, constructed and validated on experimental aggregation data for over 500 antibodies, is able to discern molecules with a high aggregation propensity as defined by experimental criteria relevant to bioprocessing and manufacturing of these molecules. Furthermore, we show how this tool can be combined with other computational approaches during early drug development to select molecules with reduced risk of aggregation and optimal developability properties.},
