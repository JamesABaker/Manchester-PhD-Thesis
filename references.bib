title = {{The TOPCONS web server for consensus prediction of membrane protein topology and signal peptides}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25969446 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4489233},
