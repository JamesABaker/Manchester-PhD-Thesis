isbn = {1097-0134},
issn = {10970134},
journal = {Proteins: Structure, Function and Bioinformatics},
keywords = {Hydrophobicity scale,Membrane proteins,Protein structure predictions,Secondary structure prediction,Topology prediction,Translocon recognition},
