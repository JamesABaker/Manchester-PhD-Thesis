doi = {10.1016/j.molcel.2013.07.013},
file = {:home/james/Documents/Mendeley Desktop/Feige, Hendershot - 2013 - Molecular Cell - Quality control of integral membrane proteins by assembly-dependent membrane integration.pdf:pdf},
