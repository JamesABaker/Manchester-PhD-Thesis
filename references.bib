journal = {Molecular Membrane Biology},
keywords = {Designed transmembrane peptides,Hydrophobic mismatch,Interfacial anchoring,Model membranes,Protein-lipid interactions},
language = {en},
mendeley-groups = {PhD manuscript},
month = {jan},
number = {4},
pages = {271--284},
pmid = {14578043},
publisher = {Informa UK Ltd UK},
title = {{Protein-lipid interactions studied with designed transmembrane peptides: Role of hydrophobic matching and interfacial anchoring (Review)}},
