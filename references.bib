journal = {Proteins: Structure, Function and Bioinformatics},
keywords = {Hydrophobicity scale,Membrane proteins,Protein structure predictions,Secondary structure prediction,Topology prediction,Translocon recognition},
mendeley-groups = {PhD manuscript},
month = {sep},
number = {9},
pages = {2190--2198},
pmid = {24753217},
title = {{Why is the biological hydrophobicity scale more accurate than earlier experimental hydrophobicity scales?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24753217},
volume = {82},
year = {2014}
}
@article{Pogozheva2013,
