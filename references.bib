keywords = {Amino Acid Sequence,Bacillus subtilis,Bacillus subtilis: genetics,Cyanobacteria,Cyanobacteria: genetics,DNA, Bacterial,DNA, Bacterial: genetics,Endonucleases,Endonucleases: genetics,Endonucleases: metabolism,Escherichia coli,Escherichia coli: genetics,Genome, Bacterial,Helix-Turn-Helix Motifs,Humans,Models, Molecular,Molecular Sequence Data,Open Reading Frames,Phylogeny,Sequence Homology, Amino Acid,Transformation, Genetic},
language = {en},
month = {jan},
number = {2},
pages = {75--82},
pmid = {10382965},
publisher = {Oxford University Press},
title = {{Putative Mechanism of Natural Transformation as Deduced from Genome Data}},
url = {http://dnaresearch.oxfordjournals.org/content/6/2/75.abstract},
volume = {6},
year = {1999}
}
@article{Daiyasu2001,
abstract = {Recently, the zinc metallo-hydrolase family of the $\beta$-lactamase fold has grown quite rapidly, accompanied by the accumulation of sequence and structure data. The variety of the biological functions of the family is higher than expected. In addition, the members often have mosaic structures with additional domains. The family includes class B $\beta$-lactamase, glyoxalase II, arylsulfatase, flavoprotein, cyclase/dehydrase, an mRNA 3′-processing protein, a DNA cross-link repair enzyme, a DNA uptake-related protein, an alkylphosphonate uptake-related protein, CMP-N-acetylneuraminate hydroxylase, the romA gene product, alkylsulfatase, and insecticide hydrolases. In this minireview, the functional and structural varieties of the growing protein family are described. {\textcopyright} 2001 Federation of European Biochemical Societies. Published by Elsevier Science B.V. All rights reserved.},
author = {Daiyasu, Hiromi and Osaka, Kazuya and Ishino, Yoshizumi and Toh, Hiroyuki},
