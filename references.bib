file = {:home/jamesb/Documents/Mendeley Desktop/{\"{O}}jemalm et al. - 2016 - Energetics of side-chain snorkeling in transmembrane helices probed by nonproteinogenic amino acids.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
month = {sep},
