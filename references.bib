title = {{SRbeta coordinates signal sequence release from SRP with ribosome binding to the translocon.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11331598 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC125438},
volume = {20},
year = {2001}
}
@article{Hauser1995,
abstract = {Translocation of proteins across the endoplasmic reticulum membrane is initiated by the signal recognition particle (SRP), a cytoplasmic ribonucleoprotein complex consisting of a 7S RNA and six polypeptides. To investigate the functions of the SRP components, we have tested the activities of several SRP subparticles. We show that the SRP GTPase (SRP54) alone binds a signal sequence and discriminates it from a non-signal sequence. Although SRP54 alone is unable to promote translocation, SRP54 in a complex with SRP RNA is both necessary and sufficient to promote translocation of an elongation-arrested nascent protein in a GTP-regulated manner. For co-translational translocation, additional SRP components are required. We discuss the implications of our results for the function of the Escherichia coli SRP which is homologous to the SRP54/SRP-RNA complex.},
author = {Hauser, S and Bacher, G and Dobberstein, B and L{\"{u}}tcke, H and Demel, R. and de Kruijff, B. and Oudega, B. and Luirink, J. and Sinning, I.},
doi = {10.1093/emboj/19.4.531},
