journal = {Physiological Reviews},
keywords = {Animals,Chloride Channels,Chloride Channels: chemistry,Chloride Channels: physiology,GABA,GABA: chemistry,GABA: physiology,Glycine,Glycine: chemistry,Glycine: physiology,Humans,Protein Structure,Receptors,Tertiary},
