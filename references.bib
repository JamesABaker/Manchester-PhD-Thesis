file = {:home/james/Documents/Mendeley Desktop/Komar et al. - 2015 - Methods in Enzymology - ACEMBLing a multiprotein transmembrane complex The functional secyeg-secdf-yajc-yidc holot.pdf:pdf},
isbn = {9780128015216},
issn = {15577988},
journal = {Methods in Enzymology},
keywords = {ACEMBL Combinatorial DNA assembly,Holotranslocon,Membrane protein insertion,Membrane proteins,Multiprotein complex,Protein secretion,Protein-conducting channel,Recombinant production,Translocation},
month = {jan},
pages = {23--49},
pmid = {25857776},
publisher = {Academic Press},
title = {{ACEMBLing a multiprotein transmembrane complex: The functional secyeg-secdf-yajc-yidc holotranslocon protein secretase/insertase}},
url = {https://www.sciencedirect.com/science/article/pii/S0076687914001414},
volume = {556},
