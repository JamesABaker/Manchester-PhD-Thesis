@article{Oger2013,
abstract = {Microbes often face contrasted and fluctuating environmental conditions, to which they need to adapt or die. Because membranes play a central role in regulating fluxes inward and outward from the cells, maintaining the appropriate structure of the membrane is crucial to maintain cellular integrity and functions. This is achieved in bacteria and eucarya by a modification of the membrane lipid compositions, a strategy termed homeoviscous adaptation. We review here evidence for homeoviscous adaptation in Archaea, and discuss the limits of this strategy and our knowledge in this very peculiar domain of life. ?? 2013 Elsevier B.V. All rights reserved.},
