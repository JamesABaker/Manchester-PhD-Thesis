issn = {00222836},
journal = {Journal of Molecular Biology},
mendeley-groups = {PhD manuscript},
month = {may},
number = {1},
pages = {105--132},
pmid = {7108955},
title = {{A simple method for displaying the hydropathic character of a protein}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0022283682905150},
volume = {157},
year = {1982}
}
