journal = {PLoS Computational Biology},
keywords = {Animals,Computational Biology,Computational Biology: methods,Databases, Protein,Humans,Markov Chains,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: classification,Pattern Recognition, Automated,Protein Folding,Protein Sorting Signals,Protein Structure, Tertiary,Proteins,Proteins: chemistry,Proteins: classification,Reproducibility of Results,Sequence Homology, Amino Acid},
