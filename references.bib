issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Deoxyribose,Models, Molecular,Motion,Nucleic Acid Conformation,Oligodeoxyribonucleotides,Oligonucleotides,Structure},
mendeley-tags = {Structure},
month = {apr},
number = {4},
pages = {2179--2183},
pmid = {6941276},
title = {{Structure of a B-DNA dodecamer: conformation and dynamics.}},
