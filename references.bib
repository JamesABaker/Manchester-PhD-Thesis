journal = {Biochimica et Biophysica Acta - Biomembranes},
keywords = {Dimerization,Hetero-dimer,Membrane protein,Protein folding,Transmembrane helix},
number = {2},
pages = {1--3},
pmid = {27915045},
title = {{In vivo selection of heterotypically interacting transmembrane helices: Complementary helix surfaces, rather than conserved interaction motifs, drive formation of transmembrane hetero-dimers}},
