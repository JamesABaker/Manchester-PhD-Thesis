issn = {1432-1424},
journal = {The Journal of Membrane Biology},
keywords = {mediated interactions,membrane proteins {\'{a}} lipids,membrane-,structure {\'{a}} protein function,{\'{a}} protein,{\'{a}} protein dynamics {\'{a}}},
mendeley-groups = {PhD manuscript},
pages = {611--640},
pmid = {26063070},
title = {{Membrane Protein Structure, Function, and Dynamics: a Perspective from Experiments and Theory}},
url = {http://link.springer.com/10.1007/s00232-015-9802-0$\backslash$nhttp://link.springer.com/article/10.1007/s00232-015-9802-0/fulltext.html},
