arxivId = {NIHMS150003},
author = {Shahidullah, Khurshida and London, Erwin},
doi = {10.1016/j.jmb.2008.04.026},
eprint = {NIHMS150003},
file = {:home/jamesb/Documents/Mendeley Desktop/Shahidullah, London - 2008 - Effect of Lipid Composition on the Topography of Membrane-Associated Hydrophobic Helices Stabilization of T.pdf:pdf},
isbn = {6316328575},
issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {fluorescence,fluorescence quenching,membrane proteins,phosphatidylglycerol,phosphatidylserine},
mendeley-groups = {PhD manuscript},
number = {4},
pages = {704--718},
