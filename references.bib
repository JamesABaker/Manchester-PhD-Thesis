journal = {Bioinformatics},
keywords = {Algorithms,Computational Biology,Evolution,Membrane Proteins,Membrane Proteins: chemistry,Molecular,Protein,Protein Conformation,Protein: methods,Reproducibility of Results,Sequence Analysis},
mendeley-groups = {PhD manuscript,Bioinformatic tools},
month = {mar},
