issn = {13674803},
journal = {Bioinformatics},
keywords = {Computational Biology,Computational Biology: methods,Databases, Protein,Markov Chains,Neural Networks (Computer),Protein Conformation,Proteins,Proteins: chemistry},
mendeley-groups = {PhD manuscript},
number = {18},
