title = {{The Sixth Transmembrane Segment Is a Major Gating Component of the TMEM16A Calcium-Activated Chloride Channel}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29478917},
volume = {97},
year = {2018}
}
