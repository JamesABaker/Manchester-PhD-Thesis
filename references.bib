keywords = {Adolescent,Adult,Aged,Anemia, Aplastic,Anemia, Aplastic: epidemiology,Anemia, Aplastic: virology,Asian Continental Ancestry Group,Asian Continental Ancestry Group: statistics {\&} num,Child,China,China: epidemiology,Circoviridae,Circoviridae: genetics,DNA, Viral,DNA, Viral: genetics,Evolution, Molecular,Female,Hepatitis Antibodies,Hepatitis Antibodies: blood,Hepatitis, Viral, Human,Hepatitis, Viral, Human: epidemiology,Hepatitis, Viral, Human: virology,High-Throughput Nucleotide Sequencing,Humans,Male,Middle Aged,Molecular Sequence Data,Parvoviridae,Parvoviridae: genetics,Phylogeny,Prevalence,Risk Factors,Seroepidemiologic Studies,Young Adult},
