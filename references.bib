keywords = {????????-fold,Bacterial resistance,Class B ??-lactamases,Conserved motifs,Metallo-??-lactamase superfamily,Zinc binding sites},
month = {dec},
number = {12},
pages = {1686--1701},
pmid = {17597585},
title = {{Metallo-Beta-lactamases (classification, activity, genetic organization, structure, zinc coordination) and their superfamily}},
url = {http://www.sciencedirect.com/science/article/pii/S0006295207003309},
volume = {74},
