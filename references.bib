file = {:home/james/Documents/Mendeley Desktop/Sommer et al. - 2013 - Biochimica et Biophysica Acta (BBA) - Molecular Cell Research - TRAP assists membrane protein topogenesis at the.pdf:pdf},
issn = {0167-4889},
journal = {Biochimica et Biophysica Acta (BBA) - Molecular Cell Research},
