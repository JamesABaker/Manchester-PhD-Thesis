journal = {Cell},
keywords = {Cell Membrane,Cell Membrane: chemistry,Cell Membrane: metabolism,Dimerization,Humans,Membrane Lipids,Membrane Lipids: metabolism,Molecular Dynamics Simulation,Nuclear Magnetic Resonance, Biomolecular,Protein Conformation,Protein Structure, Tertiary,Receptor, Epidermal Growth Factor,Receptor, Epidermal Growth Factor: chemistry,Receptor, Epidermal Growth Factor: metabolism,Static Electricity},
mendeley-groups = {PhD manuscript},
month = {jan},
