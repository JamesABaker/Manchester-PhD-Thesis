isbn = {1089-8638 (Electronic)$\backslash$r0022-2836 (Linking)},
issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {Membrane,electrostatic interactions,helical hairpin,membrane integration,polytopic,salt bridge,translocon},
mendeley-groups = {PhD manuscript},
mendeley-tags = {Membrane,polytopic},
month = {feb},
