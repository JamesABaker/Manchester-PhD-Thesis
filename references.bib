isbn = {9788578110796},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
mendeley-groups = {Blog},
month = {apr},
number = {17},
pages = {201518377},
pmid = {25246403},
title = {{Neural correlates of the LSD experience revealed by multimodal neuroimaging}},
url = {http://www.pnas.org/cgi/content/long/113/17/4853},
volume = {113},
year = {2016}
}
