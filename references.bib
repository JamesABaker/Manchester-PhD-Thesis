journal = {Proteins: Structure, Function and Bioinformatics},
keywords = {Conformational change,DNA-binding residue,Machine learning,Protein-DNA interaction,Structural analysis,Template},
mendeley-groups = {Bioinformatic tools,ComEC manuscript},
month = {nov},
