keywords = {Allosteric Regulation,Bacillus subtilis,Bacillus subtilis: enzymology,Binding Sites,Crystallography,Endoribonucleases,Endoribonucleases: chemistry,Endoribonucleases: metabolism,Escherichia coli,Escherichia coli: enzymology,Hydrogen Bonding,Models,Molecular,Protein Conformation,Static Electricity,Structure-Activity Relationship,Thermotoga maritima,Thermotoga maritima: enzymology,X-Ray,Zinc,Zinc: metabolism,beta-Lactamases,beta-Lactamases: chemistry,beta-Lactamases: metabolism},
mendeley-groups = {ComEC manuscript},
month = {feb},
number = {7026},
pages = {657--661},
pmid = {15654328},
shorttitle = {Nature},
title = {{Structural basis for substrate binding, cleavage and allostery in the tRNA maturase RNase Z.}},
url = {http://www.nature.com/doifinder/10.1038/nature03284},
volume = {433},
year = {2005}
}
@article{Newman2011,
abstract = {RNase J is an essential enzyme in Bacillus subtilis with unusual dual endonuclease and 5???-to-3??? exonuclease activities that play an important role in the maturation and degradation of mRNA. RNase J is also a component of the recently identified "degradosome" of B. subtilis. We report the crystal structure of RNase J1 from B. subtilis to 3.0 resolution, analysis of which reveals it to be in an open conformation suitable for binding substrate RNA. RNase J is a member of the ??-CASP family of zinc-dependent metallo-??-lactamases. We have exploited this similarity in constructing a model for an RNase J1:RNA complex. Analysis of this model reveals candidate-stacking interactions with conserved aromatic side chains, providing a molecular basis for the observed enzyme activity. Comparisons of the B. subtilis RNase J structure with related enzymes reveal key differences that provide insights into conformational changes during catalysis and the role of the C-terminal domain. ?? 2011 Elsevier Ltd.},
