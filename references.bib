keywords = {Adenosine Triphosphate,Adenosine Triphosphate: metabolism,Amino Acid Sequence,Aspartic Acid,Aspartic Acid: metabolism,Base Sequence,Cross-Linking Reagents,Cross-Linking Reagents: metabolism,Cystic Fibrosis Transmembrane Conductance Regulato,Endoplasmic Reticulum,Endoplasmic Reticulum: metabolism,Glycosylation,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: genetics,Membrane Proteins: metabolism,Molecular Sequence Data,Protein Structure, Secondary,Ribosomes,Ribosomes: metabolism},
number = {2},
