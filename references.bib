file = {:home/james/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Daiyasu et al. - 2001 - Expansion of the zinc metallo-hydrolase family of the $\beta$-lactamase fold.pdf:pdf},
isbn = {0014-5793 (Print)$\backslash$n0014-5793 (Linking)},
