pmid = {25368153},
publisher = {National Academy of Sciences},
title = {{Differential gradients of interaction affinities drive efficient targeting and recycling in the GET pathway.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25368153 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4246279},
volume = {111},
year = {2014}
}
