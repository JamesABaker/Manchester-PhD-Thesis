pages = {2403--2409},
pmid = {23266354},
publisher = {Elsevier},
title = {{Post-translational translocation into the endoplasmic reticulum}},
url = {https://www.sciencedirect.com/science/article/pii/S0167488912003862},
volume = {1833},
year = {2013}
}
