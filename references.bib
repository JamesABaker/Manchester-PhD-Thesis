month = {oct},
number = {1},
pages = {99--111},
pmid = {7968523},
title = {{Characterization of comE, a late competence operon of Bacillus subtilis required for the binding and uptake of transforming DNA}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2958.1993.tb00907.x/abstract{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/7968523},
