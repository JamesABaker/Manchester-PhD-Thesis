year = {2013}
}
@article{Pace1998,
abstract = {The average globular protein contains 30{\%} ␣-helix, the most common type of secondary structure. Some amino acids occur more frequently in ␣-helices than others; this tendency is known as helix propensity. Here we derive a helix propensity scale for solvent-exposed residues in the middle positions of ␣-helices. The scale is based on measurements of helix propensity in 11 systems, including both proteins and peptides. Alanine has the highest helix propensity, and, excluding proline, glycine has the lowest, ϳ1 kcal/mol less favorable than alanine. Based on our analysis, the helix propensities of the amino acids are as follows (kcal/mol): Ala ϭ 0, Leu ϭ 0.21, Arg ϭ 0.21, Met ϭ 0.24, Lys ϭ 0.26, Gln ϭ 0.39, Glu ϭ 0.40, Ile ϭ 0.41, Trp ϭ 0.49, Ser ϭ 0.50, Tyr ϭ 0.53, Phe ϭ 0.54, Val ϭ 0.61, His ϭ 0.61, Asn ϭ 0.65, Thr ϭ 0.66, Cys ϭ 0.68, Asp ϭ 0.69, and Gly ϭ 1.},
