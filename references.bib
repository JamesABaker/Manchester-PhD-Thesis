isbn = {1089-8638 (Electronic)$\backslash$n0022-2836 (Linking)},
issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {OB-fold,X-ray crystallography,helix bundle,replication protein A,subunit interaction},
