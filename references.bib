doi = {10.1128/AEM.72.2.1316-1321.2006},
file = {:home/james/Documents/Mendeley Desktop/Kim, Carver, Kathariou - 2006 - Applied and Environmental Microbiology - Natural transformation-mediated transfer of erythromycin resist.pdf:pdf},
isbn = {0099-2240},
