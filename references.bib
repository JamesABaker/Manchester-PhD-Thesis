doi = {10.1074/jbc.273.39.25203},
file = {:home/jamesb/Documents/Mendeley Desktop/Sato, Hresko, Mueckler - 1998 - Testing the charge difference hypothesis for the assembly of a eucaryotic multispanning membrane protein.pdf:pdf},
