doi = {10.1038/s41598-017-15997-z},
file = {:home/james/Documents/Mendeley Desktop/Fowler et al. - 2017 - Scientific Reports - Features of reactive cysteines discovered through computation From kinase inhibition to enri.pdf:pdf},
