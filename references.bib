isbn = {0261-4189 (Print)$\backslash$n0261-4189 (Linking)},
issn = {02614189},
journal = {EMBO Journal},
keywords = {C-tail-anchored proteins,Membrane proteins,Mitochondria,Protein translocation},
month = {dec},
