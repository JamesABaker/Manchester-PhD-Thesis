journal = {Applied and Environmental Microbiology},
keywords = {23S,23S: genetics,Amino Acid,Animals,Anti-Bacterial Agents,Anti-Bacterial Agents: pharmacology,Antibiotics,Bacterial,Bacterial: genetics,Base Sequence,Campylobacter coli,Campylobacter coli: drug effects,Campylobacter coli: genetics,Campylobacter coli: isolation {\&} purification,DNA,Drug Resistance,Erythromycin,Erythromycin: pharmacology,Food Microbiology,Genes,Genetic,Humans,Infection,J 02862:Infection,Meat,Meat: microbiology,Medical Sciences,Microbiology Abstracts B: Bacteriology,Minimum inhibitory concentration,Molecular Sequence Data,Mutation,Point Mutation,Point mutation,RNA,Ribosomal,Sequence Homology,Sus scrofa,Sus scrofa: microbiology,Transformation,Turkeys,Turkeys: microbiology,genomics,pigs,rRNA,rRNA 23S,turkeys},
