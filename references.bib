number = {6},
pages = {295--308},
pmid = {15932905},
title = {{Transmembrane helix prediction: A comparative evaluation and analysis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15932905},
volume = {18},
year = {2005}
}
