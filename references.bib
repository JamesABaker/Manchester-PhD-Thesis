journal = {BMC Biology},
keywords = {Algorithms,Databases, Protein,Enzymes,Enzymes: chemistry,Evolution, Molecular,Humans,Membrane Proteins,Membrane Proteins: chemistry,Membrane Proteins: classification,Membrane Proteins: physiology,Membrane Transport Proteins,Membrane Transport Proteins: chemistry,Multigene Family,Phylogeny,Protein Structure, Secondary,Proteome,Proteome: chemistry,Receptors, Cell Surface,Receptors, Cell Surface: chemistry,Sequence Homology, Amino Acid,Structural Homology, Protein},
mendeley-groups = {Continuation report,PhD manuscript},
