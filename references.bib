keywords = {Bacterial Outer Membrane Proteins,Lipid Bilayers,Lipid Bilayers: chemistry,Models, Molecular,Nuclear Magnetic Resonance, Biomolecular,Phosphatidylcholines,Phosphatidylcholines: chemistry,Porins,Receptors, Virus,Receptors, Virus: chemistry,Static Electricity,Surface Properties,Thermodynamics,Tryptophan,Tryptophan: analogs {\&} derivatives,Tryptophan: chemistry,Water},
mendeley-groups = {PhD manuscript},
month = {oct},
number = {42},
pages = {14713--14718},
pmid = {9778346},
publisher = {American Chemical Society},
