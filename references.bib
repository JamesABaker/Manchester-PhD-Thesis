journal = {Nature Reviews Neuroscience},
keywords = {Animals,Cell Membrane,Cell Membrane: metabolism,Cell Membrane: physiology,Humans,Membrane Potentials,Membrane Potentials: physiology,Potassium,Potassium Channels,Potassium: metabolism,Potassium: physiology,Protein Structure,Signal Transduction,Signal Transduction: physiology,Synaptic Transmission,Synaptic Transmission: physiology,Tertiary,Tertiary: physiology,Voltage-Gated,Voltage-Gated: metabolism,Voltage-Gated: physiology},
