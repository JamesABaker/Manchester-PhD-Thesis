mendeley-groups = {PhD manuscript},
month = {jan},
number = {2},
pages = {973--978},
pmid = {1985975},
title = {{Charged residues are major determinants of the transmembrane orientation of a signal-anchor sequence}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1985975},
volume = {266},
year = {1991}
}
@article{Kida2006,
abstract = {In topogenesis of membrane proteins on the endoplasmic reticulum, the orientation of the hydrophobic transmembrane (TM) segment is influenced by the charge of the flanking amino acid residues. We assessed the function of the positive charges downstream of the hydrophobic segment using synaptotagmin II. The positive charges were systematically replaced with non-charged residues. Although the original TM segment translocated the N terminus, the topology was inverted, depending on the mutations. Orientation was affected in mutants in which 6 Lys were shifted downstream, even when the 6 Lys were 25 residues from the hydrophobic segment. The Lys was functionally replaced by Arg, but not by Asp or Glu. The timing of action during polypeptide elongation indicated that the Lys functions at the ribosome exit sites. We suggest that the commitment of the TM segment to a particular orientation is influenced by far downstream parts of the polypeptide chain and that the positive charges are decoded after exiting the ribosome.},
author = {Kida, Yuichiro and Morimoto, Fumiko and Mihara, Katsuyoshi and Sakaguchi, Masao},
doi = {10.1074/jbc.M506613200},
isbn = {0021-9258 (Print)},
